{
    "name": "Remove Youtube Watch Later Videos",
    "description": "Gives more options to removing videos from the watch later playlist",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/logo16.png",
        "32": "/images/logo32.png",
        "48": "/images/logo48.png",
        "128": "/images/logo128.png"
      }
    },
    "icons": {
        "16": "/images/logo16.png",
        "32": "/images/logo32.png",
        "48": "/images/logo48.png",
        "128": "/images/logo128.png"
    }
  }